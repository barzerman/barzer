    <stmt>
        <pat>
        <t>$</t>
        <list v="val"><n /></list> 
        </pat>
        <tran>
            <func name="mkERC">
            <func name="mkEnt"><ltrl t="string">USD</ltrl><rn v="1" /><rn v="1" /></func>
            <func name="mkRange"><var name="val" /></func>
            </func>
        </tran>
    </stmt>
    <stmt>
        <pat>
        <list v="val"><n /></list>
        <t>$</t>
        </pat>
        <tran>
            <func name="mkERC">
            <func name="mkEnt"><ltrl t="string">USD</ltrl><rn v="1" /><rn v="1" /></func>
            <func name="mkRange"><var name="val" /></func>
            </func>
        </tran>
    </stmt>
    
    <stmt>
    <pat>
        <list v="val"><n /></list>
        <t>grand</t>
    </pat>
    <tran>
        <func name="mkERC">
        <func name="mkEnt"><rn v="1" /><rn v="1" /></func>
        <func name="mkRange">
            <func name="opMult"><var name="val" /><rn v="1000" /></func>
        </func>
        </func>        
    </tran>
    </stmt>
    <stmt>
        <pat>
            <list v="val"><n /></list>
            <any><t>euro</t><t>euros</t><t>eur</t></any>
        </pat>
        <tran>
            <func name="mkERC">
            <mkent i="EUR" c="1" s="1" />
            <func name="mkRange"><var name="val" /></func>
            </func>
        </tran>
    </stmt>
    <stmt>
        <pat>
        <list v="val"><n /></list>
        <any v="curr"><t>bucks</t><t>dollars</t><t>dollar</t></any>
        </pat>
        <tran>
            <func name="mkERC">
            <func name="mkEnt"><ltrl>USD</ltrl><rn v="1" /><rn v="1" /></func>
            <func name="mkRange"><var name="val" /></func>
            </func>
        </tran>
    </stmt> 
	<stmt>
	    <pat>
	        <t>between</t>
	        <list v="curr1"><ent s="1" c="1" /></list>
	        <t>and</t>
	        <list v="curr2"><ent s="1" c="1"/></list>
	    </pat>
	    <tran>
	        <func name="mkERC"><var name="curr1" /><var name="curr2" /></func>
	    </tran>
	</stmt>    
    <stmt>
        <pat>
            <t>between</t>
            <list v="low"><n /></list>
            <t>and</t>
            <list v="high"><ent c="1" s="1" /></list>
        </pat>
        <tran>
            <func name="mkERC">
                <var name="high" />
                <func name="mkERC">
                    <func name="mkEnt"><rn v="1" /><rn v="1" /></func>
                    <func name="mkRange"><var name="low" /></func>
                </func>
            </func>
        </tran>
    </stmt>
    <stmt>
        <pat>
            <list v="range"><range t="i" /></list>
            <any><t>bucks</t><t>dollars</t><t>dollar</t></any>
        </pat>
        <tran>
            <func name="mkERC">
                <mkent i="USD" c="1" s="1" />
                <var name="range" />
            </func>
        </tran>
    </stmt>
    <stmt>
        <pat>
            <list v="range"><range t="i" /></list>
            <any><t>euro</t><t>euros</t><t>eur</t></any>
        </pat>
        <tran>
            <func name="mkERC">
                <mkent i="EUR" c="1" s="1" />
                <var name="range" />
            </func>
        </tran>
    </stmt>
    <stmt>
        <pat>
            <any>
                <t>under</t>
                <list>
                    <any><t>cheaper</t><t>less</t></any>
                    <t>than</t>
                </list>
             </any>
            <list v="high"><ent c="1" s="1" /></list>
        </pat>
        <tran>
            <func name="mkERC">
                <var name="high" />
                <func name="mkERC">
                    <func name="mkEnt"><rn v="1" /><rn v="1" /></func>
                    <func name="mkRange"><rn v="0" /></func>
                </func>
            </func>
        </tran>
    </stmt>
    <stmt>
        <pat>
            <t>cheapest</t>
        </pat>
    <tran>
        <func name="mkERC">
            <func name="mkEnt"><rn v="1" /><rn v="1" /></func> 
            <func name="mkRange"></func>
        </func>
    </tran>
    </stmt>
    <stmt>
        <pat>
            <t>most</t>
            <t>expensive</t>
        </pat>
    <tran>
        <func name="mkERC">
            <func name="mkEnt"><rn v="1" /><rn v="1" /></func> 
            <func name="mkRange"><ltrl>DESC</ltrl></func>
        </func>
    </tran>
    </stmt>
    <stmt>
        <pat>
            <list v="erc1"><ent c="1" s="1" /></list>
            <any v="contents"><ent /><list><tail><t /><t /><t /></tail></list></any>
            <list v="erc2"><ent c="1" s="1" /></list>
        </pat>
    <tran>
        <var name="contents" />
        <func name="mkERC">
            <var name="erc1" /> 
            <var name="erc2" />
        </func>
    </tran>
    </stmt>    
</stmset>