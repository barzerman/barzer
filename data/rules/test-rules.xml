<?xml version="1.0" encoding="UTF-8"?>
<stmset xmlns:xsi="http://www.w3.org/2000/10/XMLSchema-instance"
    xmlns="http://www.barzer.net/barzel/0.1">

<stmt>
    <pat>
    <any v="x">
	    <t>foo</t>
	    <t>bar</t>
	    <t>baz</t>
    </any>
    </pat>
    <tran>
	    <select v="x">
        <case l="bar"><rn v="2" /></case>
	    <case l="foo"><rn v="1" /></case>
	    <case l="baz"><rn v="3" /></case>
	    </select>
    </tran>
</stmt>

</stmset>