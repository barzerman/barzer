    <stmt>
        <pat>
            <any v="mon">
            <t>january</t><t>jan</t>
            <t>february</t><t>feb</t>
            <t>march</t><t>mar</t>
            <t>april</t><t>apr</t>
            <t>may</t>
            <t>june</t><t>jun</t>
            <t>july</t><t>jul</t>
            <t>august</t><t>aug</t>
            <t>september</t><t>sep</t>
            <t>october</t><t>oct</t>
            <t>november</t><t>nov</t>
            <t>december</t><t>dec</t>
            </any>
            <list v="day"><n l="1" h="31" /></list>
            <opt><p>,</p></opt>
            <list v="year"><n l="1900" h="2100" /></list>
        </pat>
        <tran>
            <func name="mkDate">
                <var name="day" />
                <func name="lookupMonth"><var name="mon" /></func>
                <var name="year" />
            </func>
        </tran>
    </stmt>
    
