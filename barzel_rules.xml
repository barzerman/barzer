<stmset xmlns:xsi="http://www.w3.org/2000/10/XMLSchema-instance"
    xmlns="http://www.barzer.net/barzel/0.1">

<stmt>
    <pat>
        <list v="left"><ent /></list>
        <t>and</t>
        <list v="right"><ent /></list>
    </pat>
    <tran>
        <func name="mkErcExpr">
            <ltrl>AND</ltrl>
            <var name="left" />
            <var name="right" />
        </func>
    </tran>
