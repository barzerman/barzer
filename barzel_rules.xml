    <stmt>
        <pat>
            <n l="1900" h="2100"/><p>/</p><n l="1" h="12"/><p>/</p><n l="1" h="31"/>
        </pat>
        <tran><func name="mkDate"><var pn="5" /><var pn="3" /><var pn="1" /></func></tran>
    </stmt>
